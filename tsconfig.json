{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "esnext",
    "removeComments": true,
    "moduleResolution": "bundler",
    "rootDir": "./src",
    "esModuleInterop": true,
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,

  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
